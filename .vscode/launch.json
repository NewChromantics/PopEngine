	"version": "0.2.0",
	"configurations": [
		{
			"name": "Nvidia Debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceRoot}/Build/Nvidia_Debug/PopEngineTestApp",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceRoot}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing",
					"ignoreFailures": true
				}
			],
			"preLaunchTask": "Build Nvidia Debug"
		}
	]
}